#include <array>
#include <iostream>

enum Items { health_potion, torch, arrow, end };

std::array<int> startingItems{2, 5, , 10};

int countTotalItems() {
    int itemsAmount{};
    for (int i{}; i < Items::end; i++) {
        itemsAmount += startingItems[i];
    }

    return itemsAmount;
}

int main() {
    auto itemsAmount{countTotalItems()};

    std::cout << itemsAmount;
    return 0;
}
